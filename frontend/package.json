    "svelte-check": "^3.4.3",
    "svelte-jester": "^5.0.0",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^2.1.2"
